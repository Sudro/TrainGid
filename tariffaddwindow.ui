QComboBox::drop-down {
	subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 40px;
    border-left-width: 1px;
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
	image: url(:/DownArrow3.png);
}

QComboBox::drop-down:hover {
	background-color: lightgray;
}

QComboBox QAbstractItemView {
    border: 3px solid #4DB8FF;
    color: black; /* Text color in the dropdown - purple */
    background-color: #93D4FF;
}

QScrollBar:vertical {
    border: none;
    background: white; /* Можно изменить на цвет фона таблицы или окна */
    width: 7px; /* Уменьшенная ширина */
    margin: 0px; /* Если не нужно отступление от краев */
    /*border-radius: 5px; /* Уменьшенное закругление */
}

QScrollBar::handle:vertical {
    background:  #4DB8FF;
    min-height: 7px; /* Уменьшенная минимальная длина */
    /*border-radius: 5px; /* Уменьшенное закругление */
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: white; /* Это изменит цвет фона неактивных частей ползунка */
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    width: 0px;
    height: 0px;
}

